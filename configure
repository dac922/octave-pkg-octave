  acx_blas_ok=yes
else
  BLAS_LIBS=""
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
	{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $acx_blas_ok" >&5
$as_echo "$acx_blas_ok" >&6; }
	LIBS="$save_LIBS"
fi
fi

